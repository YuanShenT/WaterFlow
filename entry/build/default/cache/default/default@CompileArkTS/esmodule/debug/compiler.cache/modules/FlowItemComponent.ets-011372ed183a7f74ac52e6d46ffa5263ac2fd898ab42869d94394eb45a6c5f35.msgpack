        .width(this.FIXED_IMAGE_HEIGHT)
        .height(this.FIXED_IMAGE_HEIGHT)
        .objectFit(ImageFit.Contain)
        .margin({ top: $r('app.float.water_flow_image_top'), bottom: $r('app.float.water_flow_image_bottom') })
